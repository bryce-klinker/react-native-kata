{
  "name": "sample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint .",
    "test:watch": "yarn test --watch",
    "calabash:install": "cd ./ios && bundle exec calabash-ios download",
    "bundle:install": "bundle install --path ./.bundle && yarn calabash:install",
    "schemes:install": "yarn react-native-schemes-manager all",
    "postinstall": "yarn bundle:install && yarn schemes:install",
    "build:ios": "bundle exec fastlane ios build_local",
    "build:android": "bundle exec fastlane android build",
    "run:ios": "yarn react-native run-ios",
    "run:android": "yarn react-native run-android"
  },
  "dependencies": {
    "react": "16.8.3",
    "react-native": "0.59.2"
  },
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/runtime": "7.4.2",
    "@react-native-community/eslint-config": "0.0.3",
    "babel-jest": "24.5.0",
    "eslint": "5.15.3",
    "jest": "24.5.0",
    "metro-react-native-babel-preset": "0.53.1",
    "react-native-schemes-manager": "1.0.5",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "xcodeSchemes": {
    "Debug": [],
    "Release": [
      "Release (Calabash)"
    ]
  }
}
